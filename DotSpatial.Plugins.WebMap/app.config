<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="webMapConfigurationSection" type="DotSpatial.Plugins.WebMap.Configuration.WebMapConfigurationSection, DotSpatial.Plugins.WebMap"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
  </startup>

  <webMapConfigurationSection>
    <Services>
      <add Key="EsriWorldHydroBasemap"/>
      <add Key="EsriHydroBaseMap"/>
      <add Key="EsriWorldStreetMap"/>
      <add Key="EsriWorldImagery"/>
      <add Key="EsriWorldTopo"/>
      <add Key="BingRoads"/>
      <add Key="BingAerial"/>
      <add Key="BingHybrid"/>
      <add Key="GoogleMap"/>
      <add Key="GoogleSatellite"/>
      <add Key="GoogleLabels"/>
      <add Key="GoogleTerrain"/>
      <add Key="YahooNormal"/>
      <add Key="YahooSatellite"/>
      <add Key="YahooHybrid"/>
      <add Key="OpenStreetMap"/>
      <add Key="WMSMap"/>
    </Services>
  </webMapConfigurationSection>
  
</configuration>
